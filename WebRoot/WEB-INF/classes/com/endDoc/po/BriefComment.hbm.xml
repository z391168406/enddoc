<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.endDoc.po">
 <class name="BriefCommentPo" table="brief_comment">
  <id column="id" name="id">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="did"/>
  <property generated="never" lazy="false" name="username"/>
  <property generated="never" lazy="false" name="content"/>
  <property generated="never" lazy="false" name="rate"/>
  <property generated="never" lazy="false" name="create_time"/>
  <property generated="never" lazy="false" name="update_time"/>
  <property generated="never" lazy="false" name="isDraft">
   <column default="0" name="isDraft" sql-type="BIT"/>
  </property>
 </class>
</hibernate-mapping>
